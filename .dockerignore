# Exclude large data directories that will be mounted from Snowflake stage
geodb/
project_results/
datastore/

# Exclude Python cache and compiled files
__pycache__/
*.py[cod]
*$py.class
*.so
.Python

# Exclude virtual environments
.venv/
venv/
ENV/
env/

# Exclude IDE and editor files
.vscode/
.idea/
*.swp
*.swo
*~
.DS_Store

# Exclude Git files
.git/
.gitignore
.gitattributes

# Exclude documentation and test files
*.md
!README.md
*.ipynb
testing.ipynb

# Exclude logs
*.log
main_pipeline.log

# Exclude environment files (sensitive data)
